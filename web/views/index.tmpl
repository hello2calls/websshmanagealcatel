<div id="layout" class="content pure-g">
  <div id="DSLAMList" class="DSLAMList pure-u-1 pure-u-md-1-4">
    <div class="list pure-g">
      {{.DSLAMList}}
    </div>
  </div>
  <div id="CardList" class="CardList pure-u-1-3 pure-u-md-1-4">
    <div class="list pure-g">
      <button class="pure-button pure-u-1">
        <span class="fa-stack fa-lg">
          <i class="imageCard fa fa-hdd-o fa-stack-2x"></i>
          <i class="imageCard fa fa-close fa-stack-2x"></i>
        </span>
        <span class="textButtonCard">NALT-J</span>
      </button>
      {{.CardList}}
    </div>
  </div>
  <div id="PortList" class="PortList pure-u-1-3 pure-u-md-1-4">
    <div class="list pure-g">
      <button class="pure-button pure-u-1">
        <span class="fa-stack fa-lg">
          <i class="imagePort fa fa-caret-square-o-right"></i>
        </span>
        <span class="textButtonPort">Chambre 1</span>
      </button>
      {{.PortList}}
    </div>
  </div>
  <div id="ServiceList" class="ServiceList pure-u-1-3 pure-u-md-1-4">
    <div class="list pure-g">
      <div class="pure-u-1-2">
        <h4>Internet</h4>
      </div>
      <div class="pure-u onoffswitch">
        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="internetSwitch" checked>
        <label class="onoffswitch-label" for="internetSwitch">
          <span class="onoffswitch-inner"></span>
          <span class="onoffswitch-switch"></span>
        </label>
      </div>
    </div>
    <div class="list pure-g">
      <div class="pure-u-1-2">
        <h4>Téléphonie</h4>
      </div>
      <div class="pure-u onoffswitch">
        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="voipSwitch" checked>
        <label class="onoffswitch-label" for="voipSwitch">
          <span class="onoffswitch-inner"></span>
          <span class="onoffswitch-switch"></span>
        </label>
      </div>
    </div>
    <div class="list pure-g">
      <div class="pure-u-1-2">
        <h4>Vidéo</h4>
      </div>
      <div class="pure-u onoffswitch">
        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="iptvSwitch" checked>
        <label class="onoffswitch-label" for="iptvSwitch">
          <span class="onoffswitch-inner"></span>
          <span class="onoffswitch-switch"></span>
        </label>
      </div>
    </div>
  </div>
</div>
